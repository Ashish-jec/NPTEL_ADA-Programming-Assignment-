# include <stdio.h>
int main()
{
	int m,n,i,j;
	int k;
	int r[250][250]; int d[250][250];int g[250][250];
	printf("Enter m and n");
	scanf ("%d %d",&m,&n);
	printf("enter r");
	for (i=0;i<n;i++)
	{
	for (j=0;j<m;j++)
	scanf("%d",&r[i,j]);
    }
    
	printf("enter d");
	for (i=0;i<n;i++)
	{
	for (j=0;j<m;j++)
	scanf("%d",&d[i,j]);
    }
	
	for(i=0;i<m*n;i++)
	{
	for(j=0;j<m*n;j++)
	g[i,j]==0;
	}
	
	for (i=0;i<m*n;i++)
	{
	for (j=0;j<m*n;j++)
	if (j+*r[i,j]<=m*n)
	{k=*r[i,j];
	*g[i,j+k]=1;}
	else
	*g[i,m*n]=1;
	if (i+*d[i,j]<=m*n)
	{k=*d[i,j];
	*g[i+k,j]=1;}
	else
	*g[m*n,j]=1;
	}
	printf("graph generated is: ");
	for(i=0;i<m*n;i++)
	{
	for(j=0;j<m*n;j++)
	printf("%d",*g[i,j]);
	}
	
}
